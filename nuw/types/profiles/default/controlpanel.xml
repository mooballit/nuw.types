<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="nuw.types"
    purge="False">

    <configlet
        title="API Settings"
        action_id="nuw.types.apisettings"
        appId="nuw.types"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@api-settings.html"
        icon_expr="string:${portal_url}/++theme++nuw.types/apitool.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

    <configlet
        title="API XML Import"
        action_id="nuw.types.apixmlimport"
        appId="nuw.types"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@apixmlimport.html"
        icon_expr="string:${portal_url}/++theme++nuw.types/apitool.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>
    
    <configlet
        title="NUW Social Settings"
        action_id="social-settings"
        appId="nuw.types"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@social-settings"
        icon_expr=""
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>
</object>
